#include "lib_funcs.h"
int main(int argc, char** argv, char** env) {
    signal(SIGINT, sigint_handler);
    signal(SIGTSTP, ststp_handler);
    args = NULL;
    _input_line_ = NULL;
    _nospace_ = NULL;
    while (1) {
        isatty(0) && write(STDOUT_FILENO, "$ ", 2) < 0 ? perror("Write") : 0;
        handle_input(args, _input_line_, _nospace_, &_cmd_count_);
        execute_command(_nospace_, args, env, _cmd_count_);
    }
    return 0;
}
